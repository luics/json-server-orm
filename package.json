{
  "name": "json-server-orm",
  "version": "0.4.12",
  "description": "",
  "author": "luics.xu@gmail.com",
  "license": "MIT",
  "main": "cjs/index.js",
  "types": "cjs/index.d.js",
  "files": [
    "cjs",
    "esm"
  ],
  "keywords": [
    "json-server",
    "orm"
  ],
  "scripts": {
    "start:server": "npx ts-node test/server/server.ts",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "test:dev": "nodemon --watch ./ --ext ts --delay 0 --exec 'run-s clear test'",
    "clear": "printf \"\\33c\\e[3J\"",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx,js}\"",
    "lint:dev": "nodemon --watch .eslintrc.json --exec \"run-s clear lint\"",
    "prepare": "rm -rf cjs esm && npm run dist:lib",
    "dist:lib": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "jsonschema": "npx typescript-json-schema 'test/server/schema.ts' '*' -o 'test/server/schema.json' --required --propOrder"
  },
  "engines": {
    "node": ">= 14.17.0",
    "npm": ">= 6.14.13"
  },
  "dependencies": {
    "ajv": "^8.6.0",
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@types/assert": "^1.5.1",
    "@types/json-server": "^0.14.3",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "json-server": "^0.16.3",
    "mocha": "^9.0.2",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^8.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^4.3.2",
    "typescript-json-schema": "^0.50.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luics/json-server-orm.git"
  },
  "bugs": {
    "url": "https://github.com/luics/json-server-orm/issues"
  },
  "homepage": "https://github.com/luics/json-server-orm#readme"
}
